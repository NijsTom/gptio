{
  "name": "gptio",
  "version": "1.0.2",
  "description": "A dead simple langchain implementation for Open AI chat models.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node examples/demo.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "check-updates": "npx npm-check-updates",
    "check-updates:minor": "npx npm-check-updates --target minor",
    "check-updates:patch": "npx npm-check-updates --target patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsidsid/gptio.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "langchain",
    "openai"
  ],
  "author": "Sid Garimella <sid@prefix.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsidsid/gptio/issues"
  },
  "homepage": "https://github.com/gsidsid/gptio#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "ansi-regex": ">=6.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "esm-ts": "^0.0.9",
    "json5": "^2.2.3",
    "openai": "^3.2.1",
    "ora": "^6.3.1",
    "safe-eval": "^0.4.1"
  }
}
